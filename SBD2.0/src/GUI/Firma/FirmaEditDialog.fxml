<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml/l"
            fx:controller="GUI.Firma.FirmaEditDialogController"
            prefHeight="300.0" prefWidth="400.0">
    <children>
        <GridPane layoutX="26.0" layoutY="22.0" prefHeight="214.0" prefWidth="482.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Nazwa"/>
                <Label text="Adres" GridPane.rowIndex="1"/>
                <Label text="NIP" GridPane.rowIndex="2"/>
                <TextField fx:id="NazwaTexfField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
                <TextField fx:id="AdresTexfField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="NIPTexfField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
            </children>
        </GridPane>
        <ButtonBar prefHeight="40.0" prefWidth="200.0" layoutX="250.0" layoutY="250.0">
            <buttons>
                <Button mnemonicParsing="false" text="OK" onMouseClicked="#buttonOKClicked"/>
                <Button mnemonicParsing="false" text="Anuluj"
                        onMouseClicked="#buttonAnulujClicked"/>
            </buttons>
        </ButtonBar>
    </children>

</AnchorPane>
