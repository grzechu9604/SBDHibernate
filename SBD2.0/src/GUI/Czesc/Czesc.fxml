<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="GUI.Czesc.CzescController"
            prefHeight="480.0" prefWidth="680.0">
    <children>
        <SplitPane dividerPositions="0.5" prefHeight="480.0" prefWidth="680.0"
                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="0.0">
            <items>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <TableView fx:id="CzesciTab" layoutY="12.0" prefHeight="466.0" prefWidth="344.0"
                                   AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="-9.0" AnchorPane.topAnchor="12.0">
                            <columns>
                                <TableColumn fx:id="OpisColumn" prefWidth="172.0" text="Nazwa"/>
                                <TableColumn fx:id="CenaZakupuColumn" prefWidth="172.0" text="Cena zakupu"/>
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <Label text="Dane"/>
                        <GridPane layoutY="47.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Label text="Kategoria" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                                <Label fx:id="KategoriaDataLabel" text="Label" GridPane.columnIndex="1"
                                       GridPane.rowIndex="0"/>
                                <Label text="Opis" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                                <Label fx:id="OpisDataLabel" text="Label" GridPane.columnIndex="1"
                                       GridPane.rowIndex="1"/>
                                <Label text="Cena zakupu" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
                                <Label fx:id="CenaZakupuDataLabel" text="Label" GridPane.columnIndex="1"
                                       GridPane.rowIndex="2"/>
                                <Label text="Cena sprzedaży" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
                                <Label fx:id="CenaSprzedazyDataLabel" text="Label" GridPane.columnIndex="1"
                                       GridPane.rowIndex="3"/>
                            </children>
                        </GridPane>
                        <ButtonBar layoutX="14.0" layoutY="361.0" prefHeight="40.0" prefWidth="200.0">
                            <buttons>
                                <Button mnemonicParsing="false" text="Nowy"
                                        onMouseClicked="#handleNewCzesc"/>
                                <Button mnemonicParsing="false" text="Edytuj"
                                        onMouseClicked="#handleEditCzesc"/>
                                <Button mnemonicParsing="false"
                                        onMouseClicked="#handleDeleteDzial" text="Usuń"/>
                                <Button mnemonicParsing="false"
                                        onMouseClicked="#handleBackToMainMenu" text="Wróć"/>
                            </buttons>
                        </ButtonBar>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>


</AnchorPane>
