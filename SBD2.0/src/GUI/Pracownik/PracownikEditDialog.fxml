<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Pracownik.PracownikEditDialogController">
    <children>
        <GridPane layoutX="26.0" layoutY="22.0" prefHeight="214.0" prefWidth="482.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Imię"/>
                <Label text="Nazwisko" GridPane.rowIndex="1"/>
                <Label text="Numer telefonu" GridPane.rowIndex="2"/>
                <Label text="Dział" GridPane.rowIndex="3"/>
                <Label text="Etat" GridPane.rowIndex="4"/>
                <TextField fx:id="ImieTexfField" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
                <TextField fx:id="NazwiskoTexfField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="NrTelefonuTexfField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <ComboBox fx:id="DzialComboBox" prefHeight="31.0" prefWidth="238.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="3"/>
                <ComboBox fx:id="EtatComboBox" prefHeight="31.0" prefWidth="237.0" GridPane.columnIndex="1"
                          GridPane.rowIndex="4"/>
            </children>
        </GridPane>
        <ButtonBar layoutX="250.0" layoutY="250.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button fx:id="OKButton" mnemonicParsing="false" onMouseClicked="#buttonOKClicked" text="OK"/>
                <Button fx:id="AnulujButton" mnemonicParsing="false" onMouseClicked="#buttonAnulujClicked"
                        text="Anuluj"/>
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
